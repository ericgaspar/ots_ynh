{
    "name": "ots",
    "id": "ots",
    "packaging_format": 1,
    "description": {
        "en": "One-Time-Secret sharing platform with a symmetric 256bit AES encryption in the browser",
        "fr": "Expliquez en *quelques* (10~15) mots l'utilité de l'app ou ce qu'elle fait (l'objectif est de donner une idée grossière pour des utilisateurs qui naviguent dans un catalogue de 100+ apps)"
    },
    "version": "0.18.1~ynh1",
    "url": "https://ots.fyi/",
    "upstream": {
        "license": " Apache-2.0 License",
        "website": "https://github.com/Luzifer/ots",
        "demo": "https://ots.fyi/",
        "admindoc": "https://yunohost.org/packaging_apps",
        "userdoc": "https://yunohost.org/apps",
        "code": "https://github.com/Luzifer/ots"
    },
    "license": " Apache-2.0 License",
    "maintainer": {
        "name": "John doe",
        "email": "john.doe@example.com"
    },
    "requirements": {
        "yunohost": ">= 4.2.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/ots",
                "default": "/ots"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
